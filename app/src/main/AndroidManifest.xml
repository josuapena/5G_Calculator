<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jptechnology.appestacaohack">
    <!-- Asking permission for internet use -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:icon="@mipmap/ic_5g"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_5g_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LanguageActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".NrArfcnActivity" />
        <activity android:name=".RecepcaoActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FrequenciaActivity" />
    </application>

</manifest>